{
  "expo": {
    "name": "Medician",
    "slug": "medician",
    "version": "1.0.5",
    "owner": "logicpop",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow users to take photos/videos for tracking symptoms.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to allow users to select photos/videos for tracking symptoms.",
        "NSFaceIDUsageDescription": "This app uses Face ID to allow users to lock their app with Face ID authentication.",
        "NSTouchIDUsageDescription": "This app uses Touch ID to allow users to lock their app with Face ID authentication."
      },
      "bundleIdentifier": "com.logicpop.medician",
      "buildNumber": "1.0.1",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.logicpop.medician",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
